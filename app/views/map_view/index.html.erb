<!-- This is just the top of the page.  -->
<div>
  <h1>Google Map</h1>
</div>

<hr>

<!-- This will be populated by the map -->
<div id='map'>
</div>

<script>
var map;
  function initMap() {
   map = new google.maps.Map(document.getElementById('map'), {
     center: {lat: -43.6532, lng: 79.3832},
     zoom: 8
   });
}
</script>
<script src="http://maps.googleapis.com/maps/api/js?key=""ENV['GOOGLE_KEY']"&callback=initMap"
async defer></script>
